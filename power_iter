def iterPower(base, exp):
    '''
    base: int or float.
    exp: int >= 0

    returns: int or float, base^exp
    '''
    # Your code here
    res = 1
    if exp == 1 :
        return base
    else:
        for i in range (1,exp+1):
            res *= base
        return res

print(iterPower(10,2))